/* componests/searchbar/searchbar.wxss */
.searchbar{
  background-color: #41be52;
  padding: 20rpx;
}

.search-navigator{
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  border-radius: 10rpx;
  background-image:url("data:image/png;base64,"); 
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 8%;
}

.search-input-group{
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  border-radius: 10rpx;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}

.search-input{
  min-height: 40rpx;
  height: 40rpx;
  font-size: 12px;
}